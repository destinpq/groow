{
  "name": "groow-api-tests",
  "version": "1.0.0",
  "description": "Cypress API tests for Groow platform",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "cypress run --spec 'cypress/e2e/**/*.cy.ts'",
    "test:auth": "cypress run --spec 'cypress/e2e/01-authentication.cy.ts'",
    "test:products": "cypress run --spec 'cypress/e2e/02-products.cy.ts'",
    "test:categories": "cypress run --spec 'cypress/e2e/03-categories.cy.ts'",
    "test:brands": "cypress run --spec 'cypress/e2e/04-brands.cy.ts'",
    "test:orders": "cypress run --spec 'cypress/e2e/05-orders.cy.ts'",
    "test:cart": "cypress run --spec 'cypress/e2e/06-cart.cy.ts'",
    "test:marketing": "cypress run --spec 'cypress/e2e/07-marketing.cy.ts'",
    "test:cms": "cypress run --spec 'cypress/e2e/08-cms.cy.ts'",
    "test:reports": "cypress run --spec 'cypress/e2e/09-reports.cy.ts'",
    "test:rfq": "cypress run --spec 'cypress/e2e/10-rfq.cy.ts'",
    "test:core": "cypress run --spec 'cypress/e2e/{01,02,03,04,05,06}*.cy.ts'",
    "test:all": "cypress run",
    "test:comprehensive": "cypress run --spec 'cypress/e2e/00-comprehensive-api-test.cy.ts'",
    "test:comprehensive:headed": "cypress run --spec 'cypress/e2e/00-comprehensive-api-test.cy.ts' --headed --browser chrome",
    "test:all-apis": "cypress run --spec 'cypress/e2e/generated/**/*.cy.ts'",
    "test:all-apis:headed": "cypress run --spec 'cypress/e2e/generated/**/*.cy.ts' --headed --browser chrome",
    "test:master": "cypress run --spec 'cypress/e2e/01-master-comprehensive-test.cy.ts'",
    "test:authenticated": "cypress run --spec 'cypress/e2e/02-authenticated-api-test.cy.ts'",
    "test:authenticated:headed": "cypress run --spec 'cypress/e2e/02-authenticated-api-test.cy.ts' --headed --browser chrome",
    "report": "cypress run --reporter json --reporter-options output=test-results.json",
    "report:open": "open cypress/reports/api-test-report.html || xdg-open cypress/reports/api-test-report.html",
    "test:fixed": "cypress run --spec \"cypress/e2e/fixed/**/*.cy.ts\"",
    "test:fixed:headed": "cypress run --spec \"cypress/e2e/fixed/**/*.cy.ts\" --headed --browser chrome",
    "test:real": "cypress run --spec \"cypress/e2e/real-backend/**/*.cy.ts\"",
    "test:real:headed": "cypress run --spec \"cypress/e2e/real-backend/**/*.cy.ts\" --headed --browser chrome"
  },
  "keywords": [
    "cypress",
    "api",
    "testing",
    "groow"
  ],
  "author": "Groow Team",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "cypress": "^13.6.0",
    "typescript": "^5.3.0"
  }
}